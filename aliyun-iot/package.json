{
  "name": "ali-iot",
  "version": "1.0.0",
  "description": "aliyun IOT server SDK for nodejs.",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "node_modules/.bin/babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "aliyun",
    "IOT"
  ],
  "dependencies": {
    "js-schema": "^1.0.1",
    "moment": "^2.21.0",
    "node-fetch": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ]
    ]
  },
  "author": "nodeweb",
  "license": "MIT"
}
