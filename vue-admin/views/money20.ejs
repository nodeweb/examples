<el-card >
  <el-row :gutter="20">
    <el-col :span="8" style="line-height: 28px;font-weight: bold">
      <i class="el-icon-setting"></i>
      <span>20元红包设置</span>
    </el-col>
    <el-col :span="8" style="text-align: center">
      <template v-if="edit20">
        <el-button type="primary" size="mini" :disabled="do20" @click="changeEdit20" icon="el-icon-edit">编辑规则</el-button>
      </template>
      <template v-else>
        <el-button type="success" size="mini" :disabled="do20" @click="saveEdit20" icon="el-icon-check">保存规则</el-button>
      </template>
    </el-col>
    <el-col  :span="8" style="text-align: right">
      <el-button type="warning" size="mini"  @click="restart20" icon="el-icon-refresh">重新发包</el-button>
    </el-col>
  </el-row>
</el-card>
