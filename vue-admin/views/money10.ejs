<el-card >
  <el-row :gutter="20">
    <el-col :span="8" style="line-height: 28px;font-weight: bold">
      <i class="el-icon-setting"></i>
      <span>10元红包设置</span>
    </el-col>
    <el-col :span="8" style="text-align: center">
        <template v-if="edit10">
          <el-button type="primary" size="mini" :disabled="do10" @click="changeEdit10" icon="el-icon-edit">编辑规则</el-button>
        </template>
        <template v-else>
          <el-button type="success" size="mini" :disabled="do10" @click="saveEdit10" icon="el-icon-check">保存规则</el-button>
        </template>
    </el-col>
    <el-col  :span="8" style="text-align: right">
      <el-button type="warning" size="mini"  @click="restart10" icon="el-icon-refresh">重新发包</el-button>
    </el-col>
  </el-row>
</el-card>
