<% include header %>


<script type="text/x-template" id="agent-template">
  <div>
    <el-table
      ref="multipleTable"
      :data="list"
      tooltip-effect="dark"
      style="width: 100%"
      v-loading.fullscreen.lock="loading"
      element-loading-background="rgba(255, 255, 255, 0.3)"
      @selection-change="handleSelectionChange">
      <el-table-column
        type="selection"
        width="55">
      </el-table-column>
      <el-table-column
        label="#"
        width="66"
        prop="ID">
      </el-table-column>
      <el-table-column
        label="注册日期"
        width="166">
        <template slot-scope="scope"><span v-html="formatDate(scope.row.CreatedAt)"/></template>
      </el-table-column>
      <el-table-column
        label="手机号码"
        width="166"
        prop="Mobile">
      </el-table-column>
      <el-table-column
        label="金额"
        width="66">
        <template slot-scope="scope"><span v-html="formatMoney(scope.row.Money)"/></template>
      </el-table-column>
      <el-table-column
        width="66"
        prop="PrevId"
        label="上级ID">
      </el-table-column>
      <el-table-column
        width="66"
        prop="Team"
        label="渠道组">
      </el-table-column>
      <el-table-column
        label="代理openid"
        prop="Uid"
        show-overflow-tooltip>
      </el-table-column>
    </el-table>
    <div style="margin-top: 20px">
      <el-pagination
        @size-change="handleSizeChange"
        @current-change="handleCurrentChange"
        :current-page="currentPage"
        :page-sizes="[100, 200, 300, 400]"
        :page-size="currentNum"
        layout="total, sizes, prev, pager, next, jumper"
        :total="total">
      </el-pagination>
    </div>
  </div>
</script>



<div id="agent">
  <agentlist></agentlist>
</div>
<script src="/static/js/agent.js"></script>

<% include footer %>
